  "rdfs:comment": {
    "de": [
      "Jh. (M.H.); Glockenturm mit Mauer und Wehrgang; Portal mit gebrochenen, mit Leisten verzierten Wänden; gallo-römische Säulen und Kapitelle."
    ],
    "en": [
      "Fortified building from the 13th century (M.H.); walled bell tower with defensive reduction; portal with broken arches decorated with mouldings; Gallo-Roman columns and capitals."
    ],
    "it": [
      "Edificio fortificato del XIII secolo (M.H.); campanile a muro con riduzione difensiva; portale con archi spezzati decorati da modanature; colonne e capitelli gallo-romani."
    ],
    "fr": [
      "Edifice fortifié du XIIIème siècle (M.H) ; clocher mur avec réduit défensif ; portail à voussures brisées ornées de moulures ; colonnetttes et chapiteaux gallo-romains."
    ],
    "nl": [
      "Vestinggebouw uit de 13e eeuw (M.H.); muurklokkentoren met verdedigingsreductie; portaal met gebroken bogen versierd met lijstwerk; Gallo-Romeinse zuilen en kapitelen."
    ],
    "es": [
      "Edificio fortificado del siglo XIII (M.H.); campanario de pared con reducción defensiva; portal con arcos rotos decorados con molduras; columnas y capiteles galo-romanos."
    ]
  },
