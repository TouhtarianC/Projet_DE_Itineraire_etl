  "rdfs:comment": {
    "de": [
      "Verkauf von Pineau des Charentes und Cognac.\r\nTäglich von 9 bis 12 Uhr und von 14 bis 19 Uhr nach Vereinbarung.\r\nBesichtigung mit Verkostung und Verkauf der Produkte nach Voranmeldung."
    ],
    "en": [
      "Sale of pineau des Charentes, cognac.\r\nEvery day from 9am to 12pm and from 2pm to 7pm by appointment.\r\nVisit with tasting and sale of products on reservation."
    ],
    "it": [
      "Vendita di pineau des Charentes, cognac.\r\nTutti i giorni dalle 9 alle 12 e dalle 14 alle 19 su appuntamento.\r\nVisita con degustazione e vendita di prodotti su prenotazione."
    ],
    "fr": [
      "Vente de pineau des Charentes, cognac.\r\nTous les jours de 9h à 12h et de 14h à 19 h sur rendez vous.\r\nVisite avec dégustation et vente des produits sur réservation."
    ],
    "nl": [
      "Verkoop van pineau des Charentes, cognac.\r\nElke dag van 9 tot 12 uur en van 14 tot 19 uur op afspraak.\r\nBezoek met proeverij en verkoop van producten op reservering."
    ],
    "es": [
      "Venta de pineau des Charentes, coñac.\r\nTodos los días de 9 a 12 horas y de 14 a 19 horas con cita previa.\r\nVisita con degustación y venta de productos bajo reserva."
    ]
  },
