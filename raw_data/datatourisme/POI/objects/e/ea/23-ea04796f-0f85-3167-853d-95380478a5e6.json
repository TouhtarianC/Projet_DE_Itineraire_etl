  "rdfs:comment": {
    "de": [
      "Das von der Straße aus sichtbare Waschhaus La Planche diente den Waschfrauen aus dem Marktflecken Bonnat und den umliegenden Dörfern."
    ],
    "en": [
      "The La Planche washhouse, visible from the road, was used by washerwomen from Bonnat and surrounding villages."
    ],
    "it": [
      "Il lavatoio La Planche, visibile dalla strada, era utilizzato dalle lavandaie di Bonnat e dei villaggi circostanti."
    ],
    "fr": [
      "Le lavoir de La Planche visible de la route, servait aux lavandières du bourg de Bonnat et des villages environnants."
    ],
    "nl": [
      "Het washuis La Planche, zichtbaar vanaf de weg, werd gebruikt door de wasvrouwen van Bonnat en de omliggende dorpen."
    ],
    "es": [
      "El lavadero de La Planche, visible desde la carretera, era utilizado por las lavanderas de Bonnat y de los pueblos de los alrededores."
    ]
  },
