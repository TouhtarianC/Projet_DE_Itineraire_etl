  "rdfs:comment": {
    "de": [
      "Im Erdgeschoss des Hèbre Museums befindet sich das Interpretationszentrum für Architektur und Kulturerbe, um Rochefort zu entdecken."
    ],
    "en": [
      "The ground floor of the Hèbre Museum hosts the Interpretation of Architecture and Heritage Centre (IAHC) to share and discover the city of Rochefort."
    ],
    "it": [
      "Il piano terra del Musée Hèbre ospita il Centre d'Interprétation de l'Architecture et du Patrimoine (CIAP) per condividere e scoprire la città di Rochefort."
    ],
    "fr": [
      "Le rez-de-chaussée du Musée Hèbre accueille le Centre d'Interprétation de l'Architecture et du Patrimoine (CIAP) pour partager et découvrir la ville de Rochefort."
    ],
    "nl": [
      "Op de begane grond van het Musée Hèbre bevindt zich het Centre d'Interprétation de l'Architecture et du Patrimoine (CIAP) om de stad Rochefort te delen en te ontdekken."
    ],
    "es": [
      "La planta baja del Museo Hèbre alberga el Centro de Interpretación de la Arquitectura y el Patrimonio (CIAP) para compartir y descubrir la ciudad de Rochefort."
    ]
  },
