  "rdfs:comment": {
    "de": [
      "Nur wenige Minuten von Angoulême entfernt bewahrt die Moulin de la Courade (1641) an einem einzigartigen Standort alle Spuren der industriellen Papierherstellung im Angoumois auf."
    ],
    "en": [
      "A few minutes from Angoulême, the Moulin de la Courade (1641) preserves on a unique site all the traces of the industrial life of the paper industry in Angoulême."
    ],
    "it": [
      "A pochi minuti da Angoulême, il Moulin de la Courade (1641) conserva in un sito unico tutte le tracce dell'industria della carta nell'Angoumois."
    ],
    "fr": [
      "A quelques minutes d'Angoulême, le Moulin de la Courade (1641) conserve sur un site unique toutes les traces de la vie industrielle papetière de l'Angoumois."
    ],
    "nl": [
      "Op enkele minuten van Angoulême bewaart de Moulin de la Courade (1641) op een unieke plek alle sporen van de papierindustrie in de Angoumois."
    ],
    "es": [
      "A pocos minutos de Angulema, el Moulin de la Courade (1641) conserva en un emplazamiento único todas las huellas de la industria papelera del Angoumois."
    ]
  },
