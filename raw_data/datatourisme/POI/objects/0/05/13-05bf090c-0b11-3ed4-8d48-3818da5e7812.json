  "rdfs:comment": {
    "de": [
      "Der Hafen von Château-d'oléron, ein unumgänglicher Ort, an dem man spazieren gehen kann, mit einer Mischung aus Austernzucht, Designerhütten, Kunstspaziergängen und traditionellen Holzbooten."
    ],
    "en": [
      "The port of Château-d'oléron, an unmissable place to stroll around, combining oyster farming, designer huts, artistic walks and traditional wooden boats."
    ],
    "it": [
      "Il porto di Château-d'oléron, un luogo da non perdere per una passeggiata, che unisce l'allevamento di ostriche, le capanne dei creatori, le passeggiate artistiche e le tradizionali imbarcazioni in legno."
    ],
    "fr": [
      "Le port du Château-d'oléron, lieu incontournable où se balader, mêlant ostréiculture, cabanes de créateurs, balades artistiques et bateaux traditionnels en bois."
    ],
    "es": [
      "El puerto de Château-d'oléron, un lugar de paseo ineludible que combina la ostricultura, las cabañas de los creadores, los paseos artísticos y los barcos tradicionales de madera."
    ],
    "nl": [
      "De haven van Château-d'oléron, een niet te missen plek voor een wandeling, met een combinatie van oesterkweek, scheppershutten, artistieke wandelingen en traditionele houten boten."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Port du Château-d'Oléron"
    ]
  },
