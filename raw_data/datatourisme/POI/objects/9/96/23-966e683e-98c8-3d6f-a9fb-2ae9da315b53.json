  "rdfs:comment": {
    "de": [
      "Automatische Schleuse linkes Ufer.\nKostenlose Zugangskarte bei den Schleusenwärtern in Castelmoron-sur-Lot, Buzet-sur Baïse und Villeneuve-sur-Lot."
    ],
    "en": [
      "Automatic lock on the right bank of the river. The lock keepers at Castelmoron-sur-Lot, Buzet-sur-Baïse and Villeneuve-sur-Lot provide free cards for access to the lock."
    ],
    "it": [
      "Blocco automatico sulla sponda sinistra.\nTessera di accesso gratuita presso i custodi di Castelmoron-sur-Lot, Buzet-sur-Baïse e Villeneuve-sur-Lot."
    ],
    "fr": [
      "Ecluse automatique rive gauche.\nCarte d'accès gratuite auprès des éclusiers de Castelmoron-sur-Lot, Buzet-sur Baïse et Villeneuve-sur-Lot."
    ],
    "es": [
      "Bloqueo automático en la orilla izquierda.\nTarjeta de acceso gratuita de los escluseros de Castelmoron-sur-Lot, Buzet-sur-Baïse y Villeneuve-sur-Lot."
    ],
    "nl": [
      "Automatische vergrendeling op de linkeroever.\nGratis toegangskaart van de sluiswachters in Castelmoron-sur-Lot, Buzet-sur-Baïse en Villeneuve-sur-Lot."
    ]
  },
