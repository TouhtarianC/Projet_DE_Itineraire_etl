  "rdfs:comment": {
    "de": [
      "Bewachter Strand vom 6. Juli bis 31. August im Fluss Dronne in Brantôme, dienstags bis sonntags von 11:30 bis 17:45 Uhr. Der Zugang ist kostenlos. Picknicktische in der Nähe."
    ],
    "en": [
      "Supervised beach (July/August) in the river Dronne in Brantôme. Free access."
    ],
    "it": [
      "Spiaggia sorvegliata dal 6 luglio al 31 agosto nel fiume Dronne a Brantôme, dal martedì alla domenica, dalle 11.30 alle 17.45. Accesso libero. Tavoli da picnic nelle vicinanze."
    ],
    "fr": [
      "Plage surveillée du 6 juillet au 31 août dans la rivière Dronne à Brantôme, du mardi au dimanche de 11h30 à 17h45. Accès gratuit. Tables de pique-nique à proximité."
    ],
    "es": [
      "Playa con servicio de vigilancia (julio/agosto) en el río Dronne en Brantôme. Acceso gratuito."
    ],
    "nl": [
      "Bewaakt strand van 6 juli tot 31 augustus in de Dronne bij Brantôme, dinsdag tot zondag, 11.30 tot 17.45 uur. Gratis toegang. Picknicktafels in de buurt."
    ]
  },
