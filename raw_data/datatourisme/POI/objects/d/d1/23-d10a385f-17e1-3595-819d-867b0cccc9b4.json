  "rdfs:comment": {
    "de": [
      "Logis aus dem 15. Jh., von Wassergräben umgeben. Innenhof mit Obst- und Gemüsegarten."
    ],
    "en": [
      "A 15th century lodging, surrounded by moats, with an interior court and garden, orchard and vegetable garden."
    ],
    "it": [
      "Dimora del XV secolo circondata da un fossato. Cortile interno con giardino, frutteto e orto. Aperto dal 4 luglio al 12 agosto dalle 10.00 alle 16.00. Visita gratuita."
    ],
    "fr": [
      "Logis XVème siècle entouré de douves. Cour intérieure avec jardin, verger et potager. Ouvert du 4 juillet au 12 août de 10h à 16h. Visite libre gratuite."
    ],
    "nl": [
      "Woning uit de vijftiende eeuw omringt door een slotgracht, binnenplaats met een tuin, boomgaard en moestuin."
    ],
    "es": [
      "Vivienda del siglo XV, rodeada de fosos. Patio interior con jardín, vergel y huerto."
    ]
  },
